#%RAML 1.0 DataType

description: Model for creating a new country-level geoblock configuration
properties:
  countryIso:
    description: ISO code of a country
    type: string
    required: true
  expirationDate:
    description: Expiration date of the blocking in the following format (YYYY-MM-DD).
    type: date-only
    required: false
